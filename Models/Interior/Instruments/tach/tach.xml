<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>tach.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>
<!--
  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   1.0 </red>
      <green> 0.2 </green>
      <blue>  0.0 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>
-->
  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>engines/engine/rpm</property>
    <interpolation> 
      <entry><ind>    0 </ind><dep>   0 </dep></entry>
      <entry><ind>  100 </ind><dep>  22 </dep></entry>
      <entry><ind>  600 </ind><dep>  53 </dep></entry>
      <entry><ind>  800 </ind><dep>  90 </dep></entry>
      <entry><ind> 1000 </ind><dep> 130 </dep></entry>
      <entry><ind> 1200 </ind><dep> 175 </dep></entry>
      <entry><ind> 1400 </ind><dep> 221 </dep></entry>
      <entry><ind> 1600 </ind><dep> 263 </dep></entry>
      <entry><ind> 1800 </ind><dep> 300 </dep></entry>
      <entry><ind> 2000 </ind><dep> 336 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

